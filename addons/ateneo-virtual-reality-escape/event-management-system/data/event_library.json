		"EventText": {
			"Name": "Radio",
			"Text": [
				{
					"Character": "News Reporter",
					"Line": "September 1983, top of the headlines: ",
					"Time": 5.2
				},
				{
					"Character": "News Reporter",
					"Line": "A newspaper publisher had its properties SEIZED this morning by local officials, on suspicion of distributing illegal media.",
					"Time": 7.7
				},
				{
					"Character": "News Reporter",
					"Line": "This comes after the newspaper,  *radio crackle* published a story implicating several government officials with the assassination of former senator Benigno Aquino last month.",
					"Time": 8.9
				},
				{
					"Character": "News Reporter",
					"Line": "Among the properties seized were several PRINTING PRESS machines, paper reams, and other printing paraphernalia.",
					"Time": 7.2
				},
				{
					"Character": "News Reporter",
					"Line": "A representative of the newspaper declared that these printing presses were not used to PRINT any illegal documents.",
					"Time": 7.0
				},
				{
					"Character": "News Reporter",
					"Line": "They decry the seizure as an attack on press freedom.",
					"Time": 3.5
				},
				{
					"Character": "News Reporter",
					"Line": "Administration officials have responded that the seizure was unrelated, ",
					"Time": 4.3
				},
				{
					"Character": "News Reporter",
					"Line": "pointing out that the RIGHT TO PUBLISH had been restored in early 1981 and that ANY INDIVIDUAL CAN PRINT without obtaining any prior license.",
					"Time": 9.5
				}
			]
		},
